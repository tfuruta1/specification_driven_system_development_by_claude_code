{
  "sdd_tdd_commands": {
    "project_management": [
      {
        "name": "/project-type",
        "description": "プロジェクトタイプの設定（新規/既存/ハイブリッド）",
        "pattern": "/project-type <new|existing|hybrid> [project-path]",
        "department": "CTO",
        "implementation": {
          "phases": ["initialization"],
          "output": ".claude/project/current-type.json"
        }
      },
      {
        "name": "/steering",
        "description": "プロジェクトステアリング - 方向性定義",
        "pattern": "/steering [project-name]",
        "department": "経営企画部",
        "professionals": ["戦略データアナリスト"],
        "implementation": {
          "phases": ["goals", "constraints", "priorities", "stakeholders"],
          "approval_required": true,
          "output": ".claude/steering/"
        }
      }
    ],
    "new_development": [
      {
        "name": "/spec-init",
        "description": "仕様初期化",
        "pattern": "/spec-init <project-name>",
        "department": "CTO",
        "implementation": {
          "phases": ["overview", "scope", "architecture"],
          "output": ".claude/specs/new/"
        }
      },
      {
        "name": "/spec-requirements",
        "description": "要件定義書作成",
        "pattern": "/spec-requirements <feature-name>",
        "department": "経営企画部",
        "professionals": ["戦略データアナリスト", "戦略プロダクトマネージャー"],
        "implementation": {
          "phases": ["functional", "non-functional", "constraints"],
          "approval_required": true,
          "output": ".claude/specs/new/requirements/"
        }
      },
      {
        "name": "/spec-design",
        "description": "技術設計書作成",
        "pattern": "/spec-design <feature-name>",
        "department": "CTO",
        "professionals": ["システムアーキテクト", "DevOpsエンジニア"],
        "implementation": {
          "phases": ["architecture", "modules", "interfaces", "data-model"],
          "approval_required": true,
          "output": ".claude/specs/new/design/"
        }
      },
      {
        "name": "/spec-tasks",
        "description": "タスク分解",
        "pattern": "/spec-tasks <feature-name>",
        "department": "CTO",
        "support": "人事部",
        "professionals": ["プロジェクトコーディネーター"],
        "implementation": {
          "phases": ["breakdown", "prioritization", "estimation"],
          "approval_required": true,
          "output": ".claude/specs/new/tasks/"
        }
      },
      {
        "name": "/spec-implement",
        "description": "実装開始",
        "pattern": "/spec-implement <task-id>",
        "department": "システム開発部",
        "support": "品質保証部",
        "implementation": {
          "phases": ["coding", "testing", "documentation"],
          "output": "source code"
        }
      }
    ],
    "existing_project": [
      {
        "name": "/analyze",
        "description": "既存プロジェクト解析",
        "pattern": "/analyze <project-path> [--generate-docs|--use-cache|--force-refresh]",
        "department": "CTO",
        "professionals": ["技術データアナリスト"],
        "implementation": {
          "phases": ["scan", "parse", "analyze", "document"],
          "cache_enabled": true,
          "output": ".claude/specs/existing/"
        }
      },
      {
        "name": "/modify-request",
        "description": "修正要求の詳細分析",
        "pattern": "/modify-request <description>",
        "department": "CTO",
        "support": "経営企画部",
        "professionals": ["戦略データアナリスト"],
        "implementation": {
          "phases": ["understanding", "impact-analysis", "risk-assessment"],
          "approval_required": true,
          "output": ".claude/modifications/analysis/"
        }
      },
      {
        "name": "/modify-requirements",
        "description": "修正要件定義書作成",
        "pattern": "/modify-requirements <modification-id>",
        "department": "経営企画部",
        "professionals": ["戦略プロダクトマネージャー"],
        "implementation": {
          "phases": ["as-is", "to-be", "acceptance-criteria"],
          "approval_required": true,
          "output": ".claude/modifications/requirements/"
        }
      },
      {
        "name": "/modify-design",
        "description": "修正技術設計書作成",
        "pattern": "/modify-design <modification-id>",
        "department": "CTO",
        "professionals": ["システムアーキテクト", "セキュリティスペシャリスト"],
        "implementation": {
          "phases": ["architecture-impact", "interface-changes", "data-changes", "test-strategy"],
          "approval_required": true,
          "output": ".claude/modifications/design/"
        }
      },
      {
        "name": "/tdd-start",
        "description": "TDD駆動開発開始",
        "pattern": "/tdd-start <modification-id>",
        "department": "品質保証部",
        "support": "システム開発部",
        "professionals": ["強化QAスペシャリスト"],
        "implementation": {
          "phases": ["red-test", "green-implement", "refactor"],
          "cycle": "iterative",
          "output": "tests and implementation"
        }
      },
      {
        "name": "/sync-docs",
        "description": "設計書同期",
        "pattern": "/sync-docs <project-path>",
        "department": "CTO",
        "professionals": ["戦略ドキュメンター"],
        "implementation": {
          "phases": ["diff-detection", "update", "validation"],
          "automatic": true,
          "output": ".claude/specs/existing/"
        }
      }
    ],
    "common_management": [
      {
        "name": "/spec-status",
        "description": "進捗状況確認",
        "pattern": "/spec-status [project-name]",
        "department": "CTO",
        "implementation": {
          "phases": ["collect", "analyze", "report"],
          "output": ".claude/progress/"
        }
      },
      {
        "name": "/spec-history",
        "description": "変更履歴表示",
        "pattern": "/spec-history [--since date]",
        "department": "CTO",
        "implementation": {
          "phases": ["retrieve", "format", "display"],
          "output": "console"
        }
      },
      {
        "name": "/validate-docs",
        "description": "整合性検証",
        "pattern": "/validate-docs <project-path>",
        "department": "品質保証部",
        "implementation": {
          "phases": ["check", "report", "auto-fix"],
          "output": ".claude/validation/"
        }
      }
    ]
  },
  "approval_gates": {
    "levels": [
      {
        "level": 1,
        "name": "User Approval",
        "targets": ["requirements", "design", "modifications"],
        "approver": "user",
        "criteria": "Business alignment"
      },
      {
        "level": 2,
        "name": "CTO Approval",
        "targets": ["technical-decisions", "quality"],
        "approver": "CTO",
        "criteria": "Technical feasibility and quality"
      },
      {
        "level": 3,
        "name": "Resource Approval",
        "targets": ["team-composition", "resource-allocation"],
        "approver": "CTO + 人事部",
        "criteria": "Resource availability"
      }
    ]
  },
  "cache_system": {
    "enabled": true,
    "storage": ".claude/cache/",
    "checksum_algorithm": "sha256",
    "ttl_minutes": 1440,
    "auto_invalidate": true
  },
  "mcp_integration": {
    "gemini_cli": {
      "check_command": "claude mcp list | grep gemini-cli",
      "fallback": "claude_native"
    },
    "o3_mcp": {
      "check_command": "claude mcp list | grep o3",
      "fallback": "claude_native"
    }
  }
}